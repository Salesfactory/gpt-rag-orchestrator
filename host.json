{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "http": {
    "routePrefix": "api",
    "maxConcurrentRequests": 100,
    "maxOutstandingRequests": 200,
    "dynamicThrottlesEnabled": false
  },
  "functionTimeout": "00:10:00"
}